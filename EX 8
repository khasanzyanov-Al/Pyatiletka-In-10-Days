#прототип 1, где необходимо найти слово в списке
from itertools import*
cnt = 1
for i in product("АБВГДЕЖЗИК", repeat=3):
    word = "".join(i)
    if word == "АЖЕ":
        print(cnt, i)
    cnt += 1

#прототип 2
from itertools import*
cnt = 0
for i in product("BCADEF", repeat=5):
    word = "".join(i)
    if "BC" not in word and "CB" not in word:
        cnt += 1
    print(cnt)
