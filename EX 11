from math import *

N = 10 + 26 + 8164
for k in range(1, 1000):
    i = ceil(log(N, 2))  # i в битах
    I = ceil(k*i/8)  # Один серийный номер в байтах
    if 835*I > 156*1024:
        print(k)
        break
    #шаблон если требуют найти минимально возможную длину серийного номера.

    from math import *

k = 68
for N in range(1, 1000):
    i = ceil(log(N, 2))  # i в битах
    I = ceil(k*i/8)  # Один серийный номер в байтах
    if 680*I >= 16*1024:
        print(N)
        break
    #шаблон если требуют найти минимально возможную мощность алфавита
    #N - Это мощность алфавита 
